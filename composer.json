{
    "name": "smottt/wideimage",
    "description": "An open-source PHP library for image manipulation",
    "homepage": "http://wideimage.sourceforge.net",
    "type": "library",
    "license": [
        "GPL-2.0",
        "LGPL-2.1"
    ],
    "version": "3.0.0-dev",
    "autoload": {
        "psr-4": {
            "WideImage\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": ["test/", "test/lib/"],
            "WideImage\\Image\\": "test/lib/Image",
            "WideImage\\Mapper\\": "test/lib/Mapper",
            "WideImage\\Operation\\": "test/lib/Operation"
        }
    },
    "require": {
        "php": "~7.4|~8.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "friendsofphp/php-cs-fixer": "~2.17"
    },
    "config": {
        "bin-dir": "bin"
    }
}
